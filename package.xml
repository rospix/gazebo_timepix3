<?xml version="1.0"?>
<package>
  <name>gazebo_timepix</name>
  <version>2.0.0</version>
  <description>This package contains implementation of Timepix detector simulator plugin for Gazebo.
    It allows the user to simulate readouts induced by radiation sources (use plugin gazebo_rad_source).
  </description>

  <maintainer email="stibipet@fel.cvut.cz">Petr Stibinger</maintainer>
  <maintainer email="tomas.baca@fel.cvut.cz">Tomas Baca</maintainer>

  <license>GPLv3</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>gazebo_ros</build_depend>
  <build_depend>gazebo_rad_msgs</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>gazebo_ros</run_depend>
  <run_depend>gazebo_rad_msgs</run_depend>

  <export>
  </export>
</package>
